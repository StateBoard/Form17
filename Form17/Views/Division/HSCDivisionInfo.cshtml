                    <td>@Html.DisplayFor(model => item.Other_SSC_Board)</td>
                    <td>@Html.DisplayFor(model => item.Date_of_Leaving_Secondary_School)</td>
                    <td>@Html.DisplayFor(model => item.Date_of_Leaving_Junior_College)</td>
                    <td>@Html.DisplayFor(model => item.Name_of_Secondary_School)</td>
                    <td>@Html.DisplayFor(model => item.Secondary_School_Village)</td>
                    <td>@Html.DisplayFor(model => item.Secondary_School_Taluka)</td>
                    <td>@Html.DisplayFor(model => item.Secondary_School_District)</td>
                    <td>@Html.DisplayFor(model => item.Secondary_School_State)</td>
                    <td>@Html.DisplayFor(model => item.Secondary_School_Index_No)</td>
                    <td>@Html.DisplayFor(model => item.Secondary_School_Udise_No)</td>
                    <td>@Html.DisplayFor(model => item.Attended_Junior_College_Yes_No)</td>
                    <td>@Html.DisplayFor(model => item.Name_of_Junior_College)</td>
                    <td>@Html.DisplayFor(model => item.Junior_College_Village)</td>
                    <td>@Html.DisplayFor(model => item.Junior_College_District)</td>
                    <td>@Html.DisplayFor(model => item.Junior_College_State)</td>
