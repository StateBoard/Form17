                    <td>@Html.DisplayFor(model => item.Debarred_From_Year)</td>
                    <td>@Html.DisplayFor(model => item.Debarred_To_Year)</td>
                    <td>@Html.DisplayFor(model => item.Secondary_School_Certificate_Yes_No)</td>
                    <td>@Html.DisplayFor(model => item.FYJC_Leaving_Certificate_Yes_No)</td>
                    <td>@Html.DisplayFor(model => item.School_Leaving_Certificate_Yes_No)</td>
                    <td>@Html.DisplayFor(model => item.School_Leaving_Duplicate_Certificate_Yes_No)</td>
                    <td>@Html.DisplayFor(model => item.Unique_ID_Payment)</td>
                    <td>@Html.DisplayFor(model => item.Selected_Language)</td>
                </tr>
            }
        </table>
        <br />
        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount @Html.PagedListPager(Model, Page_No => Url.Action("HSCDivisionInfo", new { Page_No, Sorting_Order = ViewBag.CurrentSortOrder, Filter_Value = ViewBag.FilterValue }))
    </div>
</div>