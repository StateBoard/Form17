                    <td>@Html.DisplayFor(model => item.Junior_College_Udise_No)</td>
                    <td>@Html.DisplayFor(model => item.Junior_College_Stream)</td>
                    <td>@Html.DisplayFor(model => item.FYJC_Passing_Month)</td>
                    <td>@Html.DisplayFor(model => item.FYJC_Passing_Year)</td>
                    <td>@Html.DisplayFor(model => item.FYJC_Passing_Status)</td>
                    <td>@Html.DisplayFor(model => item.Division)</td>
                    <td>@Html.DisplayFor(model => item.District_of_Form_Submission)</td>
                    <td>@Html.DisplayFor(model => item.Taluka_of_Form_Submission)</td>
                    <td>@Html.DisplayFor(model => item.Stream_of_Form)</td>
                    <td>@Html.DisplayFor(model => item.Medium_of_Form)</td>
