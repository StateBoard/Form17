@*<script src="~/Design/layout/scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Design/layout/scripts/jquery.validate.min.js"></script>
    <script src="~/Design/layout/scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Design/Project.js"></script>
