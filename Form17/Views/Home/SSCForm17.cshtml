<script type="text/javascript">
    $(document).ready(function () {
        $('li').removeClass('active');
        $($('#Application')).addClass(' active');
        $("#btnSubmit").click(function (e) {
            e.preventDefault();
            if ($("#frm_form17").valid()) {
                var formData = new FormData($(frm_form17)[0]);
                $.ajax({
                    url: "../Validation/SSCForm17Validation",
                    type: "POST",
                    data: formData,
                    processData: false,
                    contentType: false,
                    beforeSend: function () {
                        $("#frm_form17 :input").prop("disabled", true);
                        $(".rounded").css("filter", "brightness(75%)");
                        Showloader();
                    },
                    complete: function () {
                        Hideloader();
                        $("#frm_form17 :input").prop("disabled", false);
                        $(".rounded").css("filter", "brightness(100%)");
                    },
                    success: function (Data) {
                        if (Data.Result) {
                            Confirm_Submit_SSCForm17(formData, Data.Dialog, Data.DialogDescription);
                        }
                        else {
                            Error_Alert(Data.Message);
                        }
                    },
                    error: function (e1, e2, e3) {
                        Error_Alert(Data.Message);
                    }
                });
            }
            else {
                $('.input-validation-error').filter(":first").focus()
            }
        });

        $("#District").change(function () {
            $("#Taluka").empty();
            $.ajax({
                type: 'POST',
                url: '@Url.Action("getTalukaList")',
                dataType: 'json',
                data: { distCode: $("#District").val() },
                beforeSend: function () {
                    $("#frm_form17 :input").prop("disabled", true);
                    $(".rounded").css("filter", "brightness(75%)");
                    Showloader();
                },
                complete: function () {
                    Hideloader();
                    $("#frm_form17 :input").prop("disabled", false);
                    $(".rounded").css("filter", "brightness(100%)");
                },
                success: function (Data) {
                    for (var item in Data) {
                      $("#Taluka").append('<option value="' + Data[item].Value + '">' + Data[item].Text + '</option>');
                    }
                },
                error: function (ex) {
                    alert('Failed.' + ex);
                }
            });
            return false;
        });

        $("#District_of_Form_Submission").change(function () {
            $("#Taluka_of_Form_Submission").empty();
            if ($("#District_of_Form_Submission").val() != "0") {
                $("#Taluka_of_Form_Submission").prop("disabled", false);
                $("#Medium_of_Form").prop("disabled", true);
                $("#School_of_Form_Submission").prop("disabled", true);
            }
            else if ($("#District_of_Form_Submission").val() == 0) {
                $("#Taluka_of_Form_Submission").prop("disabled", true);
                $("#Medium_of_Form").prop("disabled", true);
                $("#School_of_Form_Submission").prop("disabled", true);
            }
            $("#Medium_of_Form").empty();
            $("#Medium_of_Form").append('<option value="0">-Select Medium-</option>');
            $("#School_of_Form_Submission").empty();
            $("#School_of_Form_Submission").append('<option value="0">-Select School-</option>');
            document.getElementById("Index_No_of_School").value = "";
            $.ajax({
                type: 'POST',
                url: '@Url.Action("getTalukaList")',
                dataType: 'json',
                data: { distCode: $("#District_of_Form_Submission").val() },
                beforeSend: function () {
                    $("#frm_form17 :input").prop("disabled", true);
                    $(".rounded").css("filter", "brightness(75%)");
                    Showloader();
                },
                complete: function () {
                    Hideloader();
                    $("#frm_form17 :input").prop("disabled", false);
                    $(".rounded").css("filter", "brightness(100%)");
                },
                success: function (Data) {
                    for (var item in Data) {
                        $("#Taluka_of_Form_Submission").append('<option value="' + Data[item].Value + '">' + Data[item].Text + '</option>');
                    }
                },
                error: function (ex) {
                    alert('Failed.' + ex);
                }
            });
            return false;
        });

        $("#Taluka_of_Form_Submission").change(function () {
            $("#Medium_of_Form").empty();
            if ($("#Taluka_of_Form_Submission").val() != "0") {
                $("#Medium_of_Form").prop("disabled", false)
                $("#School_of_Form_Submission").prop("disabled", true);
            }
            else if ($("#Taluka_of_Form_Submission").val() == "0") {
                $("#Medium_of_Form").prop("disabled", true)
                $("#School_of_Form_Submission").prop("disabled", true);
            }
            $("#School_of_Form_Submission").empty();
            $("#School_of_Form_Submission").append('<option value="0">-Select School-</option>');
            document.getElementById("Index_No_of_School").value = "";
            $.ajax({
                type: 'POST',
                url: '@Url.Action("getSSCMediumList")',
                dataType: 'json',
                data: {
                    distCode: $("#District_of_Form_Submission").val(),
                    talCode: $("#Taluka_of_Form_Submission").val()
                },
                beforeSend: function () {
                    $("#frm_form17 :input").prop("disabled", true);
                    $(".rounded").css("filter", "brightness(75%)");
                    Showloader();
                },
                complete: function () {
                    Hideloader();
                    $("#frm_form17 :input").prop("disabled", false);
                    $(".rounded").css("filter", "brightness(100%)");
                },
                success: function (Data) {
                    for (var item in Data) {
                        $("#Medium_of_Form").append('<option value="' + Data[item].Value + '">' + Data[item].Text + '</option>');
                    }
                },
                error: function (ex) {
                    alert('Failed.' + ex);
                }
            });
            return false;
        });

        $("#Medium_of_Form").change(function () {
            $("#School_of_Form_Submission").empty();
            if ($("#Medium_of_Form").val() != "0") {
                $("#School_of_Form_Submission").prop("disabled", false  );
            }
            else if ($("#Medium_of_Form").val() == "0") {
                $("#School_of_Form_Submission").prop("disabled", true);
            }
             document.getElementById("Index_No_of_School").value = "";
            $.ajax({
                type: 'POST',
                url: '@Url.Action("getSchoolList")',
                dataType: 'json',
                data: {
                    distCode: $("#District_of_Form_Submission").val(),
                    talCode: $("#Taluka_of_Form_Submission").val(),
                    medium: $("#Medium_of_Form").val()
                },
                beforeSend: function () {
                    $("#frm_form17 :input").prop("disabled", true);
                    $(".rounded").css("filter", "brightness(75%)");
                    Showloader();
                },
                complete: function () {
                    Hideloader();
                    $("#frm_form17 :input").prop("disabled", false);
                    $(".rounded").css("filter", "brightness(100%)");
                },
                success: function (Data) {
                    for (var item in Data) {
                        $("#School_of_Form_Submission").append('<option value="' + Data[item].Value + '">' + Data[item].Text + '</option>');
                    }
                },
                error: function (ex) {
                    alert('Failed.' + ex);
                }
            });
            return false;
         });

        $("#School_of_Form_Submission").change(function () {
            $("#Index_No_of_School").val($("#School_of_Form_Submission").val());
            return false;
        });

        $("#State").change(function () {
            var item = $(this);
            if (item.val() != "0" && item.val() != "Maharashtra") {
                $("#Domicile").show();
            }
            else {
                $("#Domicile").hide();
            }
        });

        $("#Whether_Handicap").change(function () {
            var item = $(this);
            if (item.val() == "Yes") {
                $("#HandicapCategory").show();
                $("#Handicap_Category")[0].selectedIndex = 0;
                $("#HandicapLetter").show();
            }
            else {
                $("#HandicapCategory").hide();
                $("#HandicapLetter").hide();
                $("#Handicap_Category")[0].selectedIndex = 0;
            }
        });

        $("#Declaration_Yes_No").change(function () {
            var item = $(this);
            if (item.val() == "Yes") {
                $("#debarredYes").show();
            }
            else {
                $("#debarredYes").hide();
            }
        });

        $("#Leaving_Certificate_Yes_No").change(function () {
            var item = $(this);
            if (item.val() == "No") {
                $("#Duplicate_Leaving_Certificate_Yes_No")[0].selectedIndex = 0;
                $("#Duplicate_LC_Yes_No").show();
                $("#LC").hide();
            }
            else if (item.val() == "Yes") {
                $("#Duplicate_Leaving_Certificate_Yes_No")[0].selectedIndex = 0;
                $("#Duplicate_LC_Yes_No").hide();
                $("#LC").show();
            }
            else {
                $("#Duplicate_Leaving_Certificate_Yes_No")[0].selectedIndex = 0;
                $("#Duplicate_LC_Yes_No").hide();
                $("#LC").hide();
            }
        });

        $("#Duplicate_Leaving_Certificate_Yes_No").change(function () {
            var item = $(this);
            if ($("#Leaving_Certificate_Yes_No").val() == "No" && (($("#Duplicate_Leaving_Certificate_Yes_No").val() == "No") || ($("#Duplicate_Leaving_Certificate_Yes_No").val() == "0"))){
                $("#LC").hide();
            }
            else {
                $("#LC").show();
            }
        });
        $("#ReadInstruction").click(function () {
            if ($(this).is(":checked")) {
                $("#Selected_Language").prop("disabled", false);
            }
            else {
                $("#Selected_Language")[0].selectedIndex = 0;
                $("#Selected_Language").prop("disabled", true);
            }
        });
        $(function () {
            $(".datepicker").datepicker({
                dateFormat: 'dd/mm/yy',
                changeMonth: true,
                changeYear: true,
                autoSize: true,
                yearRange: "1960:2022"
            });
        });
    });

    function Confirm_Submit_SSCForm17(formData, Dialog, DialogDescription) {
        Swal.fire({
            title: Dialog,
            text: DialogDescription,
            showCancelButton: true,
            confirmButtonColor: '#30d633',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes',
            cancelButtonText: 'No',
            allowOutsideClick: false,
            allowEscapeKey: false
        }).then((result) => {
            if (result.value) {
                $.ajax({
                    url: '../Home/SSCForm17',
                    type: "POST",
                    data: formData,
                    processData: false,
                    contentType: false,
                    success: function (Data) {
                        if (Data.Result) {
                            Swal.fire({
                                title: 'Please Wait',
                                text: 'Redirecting to Payment...',
                                showConfirmButton: false,
                                allowOutsideClick: false,
                                allowEscapeKey: false
                            })
                            window.location.replace(Data.RedirectURL);
                        }
                        else {
                            Error_Alert(Data.Message);
                        }
                    },
                    error: function (e1, e2, e3) {
                        Error_Alert(Data.Message);
                    }
                })
            }
        })
    }
</script>
