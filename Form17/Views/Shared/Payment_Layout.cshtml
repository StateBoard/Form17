    <!-- JAVASCRIPTS -->
    @*------------------------------Added By SADA------------------------------*@
    <script src="~/Design/layout/scripts/jquery.fitvids.min.js"></script>
    <script src="~/Design/layout/scripts/jquery.mobilemenu.js"></script>
    <script src="~/Design/layout/scripts/tabslet/jquery.tabslet.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var mybutton = document.getElementById("btngtp");
            window.onscroll = function () { scrollFunction() };
            function scrollFunction() {
                if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
                    mybutton.style.display = "block";
                } else {
                    mybutton.style.display = "none";
                }
            }
        })
        var btn = $('#btngtp');
        btn.on('click', function (e) {
            $('html, body').animate({ scrollTop: 0 }, '300');
        });

        $(document).ready(function () {
            $(function () {
                $(".datepicker").datepicker({
                    dateFormat: 'dd/mm/yy',
                    changeMonth: true,
                    changeYear: true,
                    autoSize: true,
                    yearRange: "1970:2021"
                });
            });
        });

    </script>
